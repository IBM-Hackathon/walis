import time
import requests
import json

class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self, False)

        self.tts = ALProxy('ALTextToSpeech')
        self.ttsStop = ALProxy('ALTextToSpeech', True) #Create another proxy as wait is blocking if audioout is remote

    def onLoad(self):
        self.bIsRunning = False
        self.ids = []

    def onUnload(self):
        for id in self.ids:
            try:
                self.ttsStop.stop(id)
            except:
                pass
        while( self.bIsRunning ):
            time.sleep( 0.2 )

    def onInput_onStart(self):

        data = {
           "Api": {
               "Version": "4.0"
           },
           "Request": "whatsup"
        }

        data_json = json.dumps(data)
        r = requests.post('http://snakenet.org/api/walis', data=data_json, verify=False).text

        print data_json
        print r

        latest_message = " Ups, something went wrong here! "
        r_json = None
        try:
            r_json = json.loads(r)
        except:
            latest_message = " Sorry, I could not get the the news for you! I will try later again! "

        if r_json is not None:
            try:
                latest_message = r_json['Status']['Message']
                if latest_message is None:
                    raise Exception("No news")
            except:
                latest_message = " There are no news for you today "


        print latest_message
        self.voice( " I have the following news for you:" + latest_message + " " )


    def onInput_onStop(self):
        self.onUnload() #it is recommended to reuse the clean-up as the box is stopped
        self.onStopped()

    def voice(self, p):
        self.bIsRunning = True
        try:
            sentence = "\RSPD="+ str( 90 ) + "\ "
            sentence += "\VCT="+ str( 100 ) + "\ "
            sentence += str(p)
            sentence +=  "\RST\ "
            id = self.tts.post.say(str(sentence))
            self.ids.append(id)
            self.tts.wait(id, 0)
        finally:
            try:
                self.ids.remove(id)
            except:
                pass
            if( self.ids == [] ):
                self.onStopped() # activate output of the box
                self.bIsRunning = False]]>
